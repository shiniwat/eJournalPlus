    <Grid>

        <AdornerDecorator>
            <RichTextBox IsDocumentEnabled="True" AllowDrop="True" 
                   AcceptsReturn="True" AcceptsTab="True" 
                   HorizontalAlignment="Stretch" VerticalAlignment="Stretch" 
                   Margin="4,24,4,42" HorizontalScrollBarVisibility="Auto" 
                   x:Name="_textArea" VerticalScrollBarVisibility="Auto" FontFamily="MS Gothic">
                <RichTextBox.ContextMenu>
                    <ContextMenu>
                        <MenuItem x:Name="_mi_TextAreaContextMenu_FontListHeader" Header="フォント"/>
                        <MenuItem x:Name="_mi_TextAreaContextMenu_FontSizeHeader" Header="フォントサイズ"/>
                        <MenuItem x:Name="_mi_TextAreaContextMenu_AddCommentHeader" 
                              IsEnabled="False" Header="コメント追加" Click="OnAddCommentToSelection"/>
                    </ContextMenu>
                </RichTextBox.ContextMenu>
            </RichTextBox>
        </AdornerDecorator>

        <ToolBar ToolBarTray.IsLocked="True" HorizontalAlignment="Left" Margin="8,0,100,1" 
                 VerticalAlignment="Bottom" Height="42" x:Name="_tb_ToolBox" Background="{x:Null}">

            <Button Margin="1,0,1,0" Width="25" Height="30" 
                    x:Name="_b_Bold" Click="OnSetBold"
                    Template="{DynamicResource MTB_bold}">
                <Button.ToolTip>
                    <StackPanel>
                        <Label Padding="1,5,1,1" FontSize="12" FontWeight="Bold">太字</Label>
                    </StackPanel>
                </Button.ToolTip>
            </Button>

            <Button Margin="1,0,1,0" Width="25" Height="30" 
                    x:Name="_b_Italic" Click="OnSetItalic"
                    Template="{DynamicResource MTB_italic}">
                <Button.ToolTip>
                    <StackPanel>
                        <Label Padding="1,5,1,1" FontSize="12" FontWeight="Bold">斜字</Label>
                    </StackPanel>
                </Button.ToolTip>
            </Button>
            
            <Button Margin="1,0,1,0" Width="25" Height="30" 
                    x:Name="_b_Underline" Click="OnSetUnderline"
                    Template="{DynamicResource MTB_underline}">
                <Button.ToolTip>
                    <StackPanel>
                        <Label Padding="1,5,1,1" FontSize="12" FontWeight="Bold">下線</Label>
                    </StackPanel>
                </Button.ToolTip>
            </Button>
            
            <Button Margin="1,0,1,0" Width="25" Height="30" Click="onFormat_Hyperlink"
                    Template="{DynamicResource MB_hyperlink}">
                <Button.ToolTip>
                    <StackPanel>
                        <Label Padding="1,5,1,1" FontSize="12" FontWeight="Bold">ハイパーリンク</Label>
                    </StackPanel>
                </Button.ToolTip>
            </Button>
            
            <Button Margin="1,0,1,0" Width="25" Height="30" 
                    x:Name="_b_AlignLeft" Click="OnAlignLeft"
                    Template="{DynamicResource MTB_leftalign}">
                <Button.ToolTip>
                    <StackPanel>
                        <Label Padding="1,5,1,1" FontSize="12" FontWeight="Bold">左寄せ</Label>
                    </StackPanel>
                </Button.ToolTip>
            </Button>
            
            <Button Margin="1,0,1,0" Width="25" Height="30" 
                    x:Name="_b_AlignCenter" Click="OnAlignCenter"
                    Template="{DynamicResource MTB_centeralign}">
                <Button.ToolTip>
                    <StackPanel>
                        <Label Padding="1,5,1,1" FontSize="12" FontWeight="Bold">中央寄せ</Label>
                    </StackPanel>
                </Button.ToolTip>
            </Button>
            
            <Button Margin="1,0,1,0" Width="25" Height="30" 
                    x:Name="_b_BulletList" Click="OnRunBulletList"
                    Template="{DynamicResource MTB_bulletlist}">
                <Button.ToolTip>
                    <StackPanel>
                        <Label Padding="1,5,1,1" FontSize="12" FontWeight="Bold">箇条書き</Label>
                    </StackPanel>
                </Button.ToolTip>
            </Button>
            
            <Button Margin="1,0,1,0" Width="25" Height="30" 
                    Command="ApplicationCommands.Cut" 
                    CommandTarget="{Binding ElementName=_textArea}" 
                    CommandParameter="{x:Null}"
                    Template="{DynamicResource MB_cut}" >
                <Button.ToolTip>
                    <StackPanel>
                        <Label Padding="1,5,1,1" FontSize="12" FontWeight="Bold">切り取り</Label>
                    </StackPanel>
                </Button.ToolTip>
            </Button>
            
            <Button Margin="1,0,1,0" Width="25" Height="30" Command="ApplicationCommands.Copy" 
                    CommandTarget="{Binding ElementName=_textArea}" 
                    CommandParameter="{x:Null}"
                    Template="{DynamicResource MB_copy}" >
                <Button.ToolTip>
                    <StackPanel>
                        <Label Padding="1,5,1,1" FontSize="12" FontWeight="Bold">コピー</Label>
                    </StackPanel>
                </Button.ToolTip>
            </Button>
                    
            <Button Margin="1,0,5,0" Width="25" Height="30" Command="ApplicationCommands.Paste" 
                    CommandTarget="{Binding ElementName=_textArea}" 
                    CommandParameter="{x:Null}"
                    Template="{DynamicResource MB_paste}" >
                <Button.ToolTip>
                    <StackPanel>
                        <Label Padding="1,5,1,1" FontSize="12" FontWeight="Bold">貼り付け</Label>
                    </StackPanel>
                </Button.ToolTip>
            </Button>
            
            <!--<ejpCon:ColorSwatchButton Width="55" Height="30" Margin="1,0,10,0" 
                    ActiveTextColorChanged="SetActiveTextColor" x:Name="_csb_ColorSwatchButton" />-->
        </ToolBar>
        
      <Label x:Name="_l_LinkPercentage" HorizontalAlignment="Right" Margin="0,0,20,0" VerticalAlignment="Bottom" 
               Width="Auto" Height="Auto" Content="0%" FontSize="11" Padding="0,0,0,3"/>
      <Label x:Name="_l_MojiCount" Width="Auto" Height="Auto" Content="0文字" FontSize="11" Padding="0,0,0,3"
             HorizontalAlignment="Right" Margin="0,0,59.766,0" VerticalAlignment="Bottom"/>
      <Label Width="Auto" Height="Auto" Content="/" FontSize="11" Padding="0,0,0,3" 
             HorizontalAlignment="Right" Margin="0,0,52.5,0" VerticalAlignment="Bottom"/>
    </Grid>
</UserControl>
