<Application x:Uid="Application_1" x:Class="ejpClient.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="Window1.xaml" ShutdownMode="OnMainWindowClose"
    >
    <Application.Resources>

      <ResourceDictionary x:Uid="ResourceDictionary_1">
        <ResourceDictionary.MergedDictionaries>
          <ResourceDictionary x:Uid="ResourceDictionary_2" Source="Resources\EjpLibDictionary.xaml"/>
          <ResourceDictionary x:Uid="ResourceDictionary_3" Source="Resources\EjsBridgeDictionary.xaml"/>
          <ResourceDictionary x:Uid="ResourceDictionary_4" Source="Resources\AppDefaultResDict.xaml"/>
          <ResourceDictionary x:Uid="ResourceDictionary_5" Source="Resources\EjpClientDictionary.xaml"/>
          <ResourceDictionary x:Uid="ResourceDictionary_6" Source="Resources\EjpControlsDictionary.xaml"/>
        </ResourceDictionary.MergedDictionaries>
      </ResourceDictionary>
      

    </Application.Resources>
</Application>
