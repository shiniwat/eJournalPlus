    <Grid x:Uid="RootGrid" x:Name="RootGrid">
    <Grid.RowDefinitions>
      <RowDefinition x:Uid="RowDefinition_1" Height="25"/>
      <RowDefinition x:Uid="RowDefinition_2" Height="*"/>
      <RowDefinition x:Uid="RowDefinition_3" Height="25"/>
    </Grid.RowDefinitions>

        <ScrollViewer x:Uid="_SC_Scroller" x:Name="_SC_Scroller" HorizontalContentAlignment="Stretch" 
                  VerticalContentAlignment="Stretch" Grid.Row="1" 
                  HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
            <Grid x:Uid="Grid_5">
                <InkCanvas x:Uid="_IC_MapCanvas" DragOver="OnMapCanvasDragOver" MouseMove="OnHittestMapCanvas" 
                 PreviewMouseLeftButtonUp="OnMapCanvasPreviewMouseUp" 
                 PreviewMouseLeftButtonDown="OnMapCanvasPreviewMouseDown" 
                 StylusDown="OnStylusDown" StylusOutOfRange="OnStylusOutOfRange"
                 StylusUp="OnStylusUp"
                 x:Name="_IC_MapCanvas" Drop="OnMapCanvasDrop" AllowDrop="True" 
                 Width="2200" Height="1400">
                    <InkCanvas.DefaultDrawingAttributes>
                        <DrawingAttributes x:Uid="DrawingAttributes_2" Color="Red" FitToCurve="True" Height="3" StylusTip="Ellipse" Width="3" />
                    </InkCanvas.DefaultDrawingAttributes>
                </InkCanvas>

                <Canvas x:Uid="_cv_CommentSurface" x:Name="_cv_CommentSurface" MouseLeftButtonUp="_CMS_MouseLeftButtonUp" 
                        Visibility="Collapsed" Background="Transparent" 
                        MouseLeftButtonDown="_CMS_MouseLeftButtonDown"
                        ClipToBounds="True" StylusOutOfRange="_CMS_StylusOutOfRange"
                        Width="2200" Height="1400" MouseMove="_CMS_MouseMove" />
                
            </Grid>
        </ScrollViewer>

        <ToggleButton x:Uid="_B_ToggleMapLock" x:Name="_B_ToggleMapLock" Checked="OnSetMapLock" Unchecked="OnReleaseMapLock" HorizontalAlignment="Right"
                  Template="{DynamicResource MTB_maplock}" Margin="0,4,17,4" Width="85" Height="17"
                  HorizontalContentAlignment="Right" VerticalContentAlignment="Stretch">
            <ToggleButton.ToolTip>
                <StackPanel x:Uid="StackPanel_1">
                    <Label x:Uid="Label_1" Padding="1,5,1,1" FontSize="12" FontWeight="Bold">マップロック</Label>
                    <TextBlock x:Uid="TextBlock_1" Margin="5,2,3,0" Padding="1,1,1,5" FontSize="10" >ノードをレポートや<LineBreak x:Uid="LineBreak_1"/>他のマップに引用するために<LineBreak x:Uid="LineBreak_2"/>マップを固定します</TextBlock>
                </StackPanel>
            </ToggleButton.ToolTip>
        </ToggleButton>

        <ToggleButton x:Uid="_tb_ScaleLock" HorizontalContentAlignment="Right" Checked="OnSetScaleLock" Unchecked="OnReleaseScaleLock"
                  Template="{DynamicResource MTB_scalelock}" x:Name="_tb_ScaleLock" 
                  Height="22" HorizontalAlignment="Right" Width="44"
                  Margin="0,0,16,0" VerticalAlignment="Bottom" Grid.Row="2">
            <ToggleButton.ToolTip>
                <StackPanel x:Uid="StackPanel_2">
                    <Label x:Uid="Label_2" Padding="1,5,1,1" FontSize="12" FontWeight="Bold">マップビュー</Label>
                    <TextBlock x:Uid="TextBlock_2" Margin="5,2,3,0" Padding="1,1,1,5" FontSize="10" >マップ全体を表示します</TextBlock>
                </StackPanel>
            </ToggleButton.ToolTip>
        </ToggleButton>
    
